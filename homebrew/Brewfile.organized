# Python Versions
brew "python@3.9"
brew "python@3.10"
brew "python@3.12"

# Database and CLI Tools
brew "libpq", link: true
brew "pgcli"
brew "redis", restart_service: true

# System Monitoring and Utilities
brew "htop"
brew "lazydocker"
brew "neofetch"
brew "osx-cpu-temp"
