cask "talon"
cask "the-unarchiver"
cask "zoom"

# Data Processing and Geospatial Tools
brew "apache-arrow" # In-memory columnar data format
brew "gdal" # Geospatial Data Abstraction Library for reading and writing geospatial data
brew "numpy" # Numerical computing library for Python
brew "openblas" # Optimized linear algebra library used in scientific computing
brew "poppler" # PDF rendering library, commonly used for data extraction
brew "python-tabulate" # Pretty print tables for tabular data in Python

# AWS and Cloud Tools
brew "aws-vault" # Tool for securely managing AWS credentials
brew "awsume" # Tool to quickly assume AWS roles using stored credentials
brew "chamber" # Manage secrets with AWS SSM Parameter Store and Secrets Manager
brew "heroku/brew/heroku" # Heroku CLI for deploying and managing Heroku apps
brew "terraform" # Infrastructure as code tool for cloud provisioning

# Networking and Security
brew "curl" # Command-line tool for transferring data with URLs, used in network requests
brew "gnupg", link: false # GNU Privacy Guard for encryption and signing, unlinked to avoid conflicts
brew "gnutls" # GNU TLS library for secure communications
brew "gpgme" # GPG Made Easy library for accessing GPG functions programmatically
brew "libksba" # X.509 and CMS (PKCS#7) support library for cryptographic purposes
brew "libxmlsec1" # XML security library for signing and encryption
brew "mitmproxy" # Interactive HTTPS proxy for debugging, inspecting, and modifying traffic
brew "oath-toolkit" # Tool for generating one-time passwords (OTP) for secure authentication
brew "openconnect" # Open source VPN client
brew "unbound" # DNS resolver and caching server
brew "vpn-slice" # VPN route management tool

# Development Tools and Utilities
brew "docutils"          # Python documentation utilities, especially for reStructuredText
brew "entr"              # Utility for running commands when files change, useful in development workflows
brew "foreman"           # Manage Procfile-based applications, commonly used in development
brew "gh"                # GitHub CLI for interacting with GitHub from the command line
brew "grunt-cli"         # Command-line interface for the JavaScript task runner, Grunt
brew "guile"             # GNU Ubiquitous Intelligent Language for Extensions, used in Scheme programming
brew "imagemagick"       # Image processing tool for creating, editing, and converting images
brew "jmeter"            # Performance and load testing tool for analyzing web applications
brew "neovim"            # Modernized version of Vim text editor
brew "pex"               # Python package manager that creates self-contained executable .pex files
brew "phrase-cli"        # CLI tool for Phrase, a translation management tool for developers
brew "rcm"               # Tool for managing dotfiles
brew "stow"              # Symlink manager, commonly used for managing dotfiles
brew "wget"              # Utility for downloading files from the web
brew "xpdf", link: false # PDF viewer, unlinked to avoid conflicts with other PDF tools
brew "yarn"              # JavaScript package manager, commonly used with Node.js
brew "zsh"               # Z shell, a powerful shell with scripting and customization capabilities

# Command-Line Tools and Utilities
brew "gron"             # Converts JSON into a format that's more easily grep-able
brew "jd"               # JSON diff utility to view changes between JSON files
brew "jsonpp"           # JSON pretty printer for formatting JSON output
brew "shared-mime-info" # MIME type

# Graphics and Image Processing
brew "cairo"     # 2D graphics library for vector graphics and GUIs
brew "harfbuzz"  # Text shaping engine for rendering complex scripts
brew "libheif"   # Library for HEIF and AVIF image formats
brew "tesseract" # OCR engine for converting images of text into editable text

# Networking and Communication
brew "grpc"    # High-performance RPC framework for distributed applications
brew "nghttp2" # HTTP/2 protocol implementation for modern web standards

# Java and JVM Tools
brew "openjdk"    # Latest version of OpenJDK for Java development
brew "openjdk@11" # OpenJDK version 11 for compatibility with older applications

# JavaScript and Development Runtimes
brew "oven-sh/bun/bun" # JavaScript runtime, bundler, and package manager

# PDF and Document Utilities
brew "pdftk-java" # Java-based PDF toolkit for merging, splitting, and modifying PDFs

# Package Management and System Tools
brew "rpm"  # RPM package manager, useful for handling .rpm files on macOS
brew "tldr" # Simplified, community-driven man pages for common commands

